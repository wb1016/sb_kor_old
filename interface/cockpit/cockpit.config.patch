[{"op": "replace", "path": "/planetTypeToDescription/moon/1", "value": "\uc774 ^#949493;\ub2ec^white; \uc740 \uc0dd\uba85\uc774 \uc0b4\uae30 \uc801\ud569\ud558\uc9c0 \uc54a\uc73c\uba70 ^red;\ub300\uae30\uac00 \uc874\uc7ac\ud558\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4^white;. \ud558\uc9c0\ub9cc \uc5f0\ub8cc\uac00 \ub9ce\uc740 \uac83\uc73c\ub85c \ubcf4\uc785\ub2c8\ub2e4. \uc2a4\uce94\uc5d0 \ub530\ub974\uba74 \uc9c0\ud45c\uba74 \uc544\ub798\uc5d0 \ub300\ub7c9\uc758 \uc790\uc6d0\uc774 \ub9e4\uc7a5\ub418\uc5b4 \uc788\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/moon/2", "value": "\uc774 \uc870\uc6a9\ud55c ^#949493;\ub2ec^white;\uc740 ^red;\ub300\uae30\uc5d0 \uc0b0\uc18c\uac00 \uc5c6\uc2b5\ub2c8\ub2e4^white;. \ud558\uc9c0\ub9cc \ud45c\uba74 \uc544\ub798\uc5d0 \uc788\ub294 \ub3d9\uad74\ub4e4\uc740 \ub300\ub7c9\uc758 \uc5f0\ub8cc\uac00 \ud1f4\uc801\ub418\uc5b4 \uc788\uc2b5\ub2c8\ub2e4. \uacb0\ub860\uc744 \ub9d0\ud558\uc790\uba74, \uc4f8\ubaa8\ub294 \uc788\uc2b5\ub2c8\ub2e4\ub9cc \ub531\ud788 \uac00\uace0 \uc2f6\uc744 \uacf3\uc740 \uc544\ub2d9\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/toxic/0", "value": "\uc774 \ud589\uc131\uc740 ^#06ff00;\uc720\ub3c5^white;\ud569\ub2c8\ub2e4. \uacf5\uae30\ub294 \ubd80\ud328\ud558\uace0 \ubc14\ub2e4\ub294 \ub3c5\uae30\uac00 \uac00\ub4dd\ud558\uba70 ^red;\ubc29\uc0ac\ub2a5 \uc548\uc804 \uc218\uce58\ub97c^white; \uc218\ucc9c \ubc30 \ucd08\uacfc\ud569\ub2c8\ub2e4."}, {"op": "replace", "path": "/threatLevelToText/default/2", "value": "\uc704\ud611\uc801\uc778"}, {"op": "replace", "path": "/threatLevelToText/default/0", "value": "\ub0ae\uc74c"}, {"op": "replace", "path": "/threatLevelToText/default/1", "value": "\ud3c9\ubc94\ud568"}, {"op": "replace", "path": "/threatLevelToText/default/3", "value": "\uc704\ud611\uc801\uc784"}, {"op": "replace", "path": "/threatLevelToText/default/4", "value": "\ub9e4\uc6b0 \uc704\ud5d8\ud568"}, {"op": "replace", "path": "/threatLevelToText/default/5", "value": "\uadf9\ub3c4\ub85c \uc704\ud5d8\ud568"}, {"op": "replace", "path": "/threatLevelToText/default/6", "value": "\ube44\ud604\uc2e4\uc801"}, {"op": "replace", "path": "/threatLevelToText/default/7", "value": "***"}, {"op": "replace", "path": "/threatLevelToText/default/8", "value": "***"}, {"op": "replace", "path": "/threatLevelToText/default/9", "value": "***"}, {"op": "replace", "path": "/threatLevelToText/moon/0", "value": "\uc54c \uc218 \uc5c6\uc74c"}, {"op": "replace", "path": "/planetTypeToDescription/toxic/2", "value": "\uc774\uacf3\uc740 ^red;\ubc29\uc0ac\ub2a5\uc774 \ucda9\ub9cc\ud55c^white; ^#06ff00;\uc720\ub3c5\uc131^white; \ud589\uc131\uc785\ub2c8\ub2e4. \ub3c5\uae30\uac00 \uac00\ub4dd \ucc2c \ubc14\ub2e4\ub294 \uc0dd\uba85\uc5d0 \uc704\ud5d8\ud560 \uac83\uc785\ub2c8\ub2e4... \ub2e4\ub978 \ud589\uc120\uc9c0\ub3c4 \uc788\ub2e4\ub294 \uac83\uc744 \uc54c\ub824\ub4dc\ub9bd\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/ancientgateway/0", "value": "\uae30\uc6d0\uc744 \uc54c \uc218 \uc5c6\ub294 ^#571cff;\uace0\ub300\uc758 \uad00\ubb38^white;\uc785\ub2c8\ub2e4; \uc2e0\ube44\ub86d\uace0 \uac15\ub825\ud55c \ud798\uc774 \uae43\ub4e0 \uac74\ucd95\ubb3c\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/arctic/1", "value": "\ucc29\ub959 \uc9c0\uc5ed\uc740 ^red;\uadf9\ud788 \ucda5\uace0^white; \ub113\uc740 ^#00ffe4;\uc5bc\uc5b4\ubd99\ub294 \ub4ef\ud55c \ubc14\ub2e4^white;\uc758 \uc791\uc740 \uc12c\uc785\ub2c8\ub2e4. \ubd84\uc11d\uc5d0 \ub530\ub974\uba74 0.025%\uc758 \ud655\ub960\ub85c \ubb3c \uc548\uc73c\ub85c \uc624\uc804\uc1a1\ub418\uc5b4 \ub054\ucc0d\ud55c \ucd94\uc704 \uc18d\uc5d0\uc11c \uc0ac\ub77c\uc9c8 \uc218\ub3c4 \uc788\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/arctic/2", "value": "^red;\uc704\ud5d8\ud560\uc815\ub3c4\ub85c \ub0ae\uc740 \uc628\ub3c4^white;\uc758 \uc9c0\uc5ed\uc785\ub2c8\ub2e4. \ub9e4\uc6b0 \uc801\uc740 \ub3d9\ubb3c\ub4e4\uc774 \uc0b4\uace0 \uc788\uace0, \uc2dd\ubb3c \ub300\uc2e0 ^#00ffe4;\uc5bc\uc5b4\ubd99\ub294 \ub4ef\ud55c \ubc14\ub2e4^white;\uc5d0 \ubc84\ub824\uc9c4 \uc12c\ub4e4\ub9cc\uc774 \uc788\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/midnight/0", "value": "\uc774\uacf3\uc740 \ud0dc\uc591\uc774 \uc808\ub300\ub85c \ub728\uc9c0 \uc54a\ub294\ub2e4\ub294 ^#e11212;\uc554\ud751^white; \uc138\uacc4\uc785\ub2c8\ub2e4. \uadf8 \uc601\ud5a5\uc73c\ub85c \ud45c\uba74\uc740 ^red;\ubb34\uc11c\uc6b8 \uc815\ub3c4\ub85c \ucda5\uc2b5\ub2c8\ub2e4.^white; \ucc29\ub959\ud558\uae30 \uc804\uc5d0 \ubab8\uacfc \uc815\uc2e0\uc744 \uac00\ub2e4\ub4ec\ub294 \uac78 \ucd94\ucc9c\ud569\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/midnight/1", "value": "\uc774 ^#e11212;\uc554\ud751^white; \uc138\uc0c1\uc740 ^red;\uc704\ud5d8\ud560 \uc815\ub3c4\ub85c \ucda5\uc2b5\ub2c8\ub2e4.^white; \uc0dd\uc18c\ud55c \uadf8\ub9bc\uc790 \uc0dd\ubb3c\uccb4\ub4e4\uc774 \uc601\uc6d0\ud55c \ubc24\uc5d0 \uc228\uc5b4 \uc0b4\uace0 \uc788\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/magma/0", "value": "\uc774 ^red;\uc704\ud5d8\ud558\uac8c \ub728\uac70\uc6b4^white; \ud589\uc131\uc740 \ub300\ubd80\ubd84\uc774 ^#ff1818;\uc6a9\uc554^white;\uc73c\ub85c \uc774\ub8e8\uc5b4\uc838 \uc788\uc2b5\ub2c8\ub2e4. \ub2f9\uc2e0\uc758 \ud3ec\uae30\ud558\uc9c0 \uc54a\ub294 \uc815\uc2e0\uc740 \uc874\uacbd\uc2a4\ub7fd\uc9c0\ub9cc, \ubc29\ud654 \ub2a5\ub825\uc740 \uc5c6\ub2e4\ub294 \uacb0 \uba85\uc2ec\ud574\uc8fc\uc138\uc694."}, {"op": "replace", "path": "/planetTypeToDescription/magma/1", "value": "\uc774 \ud589\uc131 \ud45c\uba74\uc740 ^red;\uc704\ud5d8\ud560 \uc815\ub3c4\ub85c \ub728\uac81\uc2b5\ub2c8\ub2e4.^white; \ub9e4\uc6b0 \uc18c\ub7c9\uc758 \uc0dd\uba85\uccb4\ub9cc\uc774 \uc774 \ud589\uc131\uc758 \ub113\uc740 ^#ff1818;\uc6a9\uc554^white; \ubc14\ub2e4\uc5d0\uc11c \uc0b4\uc544\uac11\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/magma/2", "value": "\uc774 \ucc29\ub959 \uc9c0\uc5ed\uc740 ^#ff1818;\uc6a9\uc554^white;\uc758 \ubc14\ub2e4\uc5d0 \uac07\ud78c \uc791\uc740 \uc12c\uc785\ub2c8\ub2e4. \ub2f9\uc2e0\uc774 \uc774\ub7f0 ^red;\ud0c0\ub294 \ub4ef\ud55c \uc5f4\uae30^white;\ub97c \uac00\uc9c4 \uc9c0\uc5ed\uc5d0 \uac08 \uc0dd\uac01\uc744 \ud558\ub294 \uac83\uc740 \ub2f9\uc2e0\uc774 \uac00\uc9c4 \ub9c8\uc74c\uac00\uc9d0\uc758 \ub113\uc774\ub97c \ub9d0\ud574\uc8fc\ub294\uad70\uc694."}, {"op": "replace", "path": "/planetTypeToDescription/snow/0", "value": "\uc774 \ucd94\uc6b4 \ucc29\ub959 \uc9c0\uc810\uc740 ^#69f1fd;\ub208^white;\uc5d0 \ub4a4\ub36e\uc5ec \uc788\uc2b5\ub2c8\ub2e4. \ub2e4\uc591\ud55c \ub098\ubb34\uc640 \ub3d9\ubb3c\ub4e4\uc774 \ucd94\uc704\uc5d0\uc11c \uc790\ub77c\uace0 \uc788\uc2b5\ub2c8\ub2e4. \ub208\ub369\uc774 \ub9cc\ub4e4\uae30\uc5d0 \uc801\ud569\ud574 \ubcf4\uc774\ub294\uad70\uc694."}, {"op": "replace", "path": "/planetTypeToDescription/snow/1", "value": "\uc774 \ucc29\ub959 \uc9c0\uc810\uc740 \ucda5\uc9c0\ub9cc, \uc704\ud5d8\ud55c \uc218\uc900\uc740 \uc544\ub2d9\ub2c8\ub2e4. ^#69f1fd;\ub208^white;\uc774 \ub4a4\ub36e\uace0 \uc788\uc73c\uba70 \ub208\ubcf4\ub77c\uac00 \uce60 \uc218\ub3c4 \uc788\uc2b5\ub2c8\ub2e4. \ubcf4\uc628\uc744 \uc704\ud55c \ubaa8\uc790\uc640 \uc7a5\uac11\uc744 \ucd94\ucc9c\ud569\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/snow/2", "value": "\uc774\uacf3\uc740 \ube44\uad50\uc801 \ucda5\uace0 ^#69f1fd;\ub208^white; \uc73c\ub85c \ub4a4\ub36e\uc5ec \uc788\uc2b5\ub2c8\ub2e4. \ud770 \ud45c\uba74 \uc704\ub97c \ub3cc\uc544\ub2e4\ub2c8\ub294 \uc0dd\ubb3c\uccb4\ub4e4\uc774 \uc13c\uc11c\uc5d0 \uac10\uc9c0\ub429\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/garden/0", "value": "^#76fe68;\ud478\ub978^white; \uc5b8\ub355\uc73c\ub85c \uc124\uba85\ub418\ub294 \ucc29\ub959\uc9c0\uc810\uc73c\ub85c. \uc2dd\ubb3c\uacfc \ub3d9\ubb3c\ub4e4\uc774 \ub118\uce69\ub2c8\ub2e4. \ucc29\ub959 \uc9c0\uc5ed\uc73c\ub85c\uc11c\ub294 \ub9e4\uc6b0 \uc548\uc804\ud569\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/garden/1", "value": "\ucc29\ub959 \uc9c0\uc810\uc740 \ub9e4\uc6b0 \ub2e4\uc591\ud55c \uc0dd\ubb3c\ub4e4\uc774 \uac10\uc9c0\ub418\ub294 \ud3c9\ud654\ub85c\uc6b4 \uc9c0\uc5ed\uc785\ub2c8\ub2e4. \ub2f9\uc2e0\uc774 \uc4f8\ub9cc\ud55c \uc790\uc6d0\ub4e4\ub3c4 \uc788\uc2b5\ub2c8\ub2e4. ^#76fe68;\ud478\ub974\uace0 ^white; \ucf8c\uc801\ud55c \uc9c0\uc5ed\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/jungle/1", "value": "\uc774 ^#12a003;\uc815\uae00^white; \ucc29\ub959 \uc9c0\uc810\uc740 \ub9ce\uc740 \uc885\ub958\uc758 \ub3d9\uc2dd\ubb3c\ub4e4\uc774 \ubc00\uc9d1\ud558\uac8c \uac70\uc8fc\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4. \uc774 \uc9c0\uc5ed\uc740 \uac00\uce58\uc788\ub294 \uc790\uc6d0\ub4e4\uc774 \uc788\uc9c0\ub9cc ^red;\uadf9\ub3c4\ub85c \ub192\uc740 \ubc29\uc0ac\uc120^white;\uc744 \ubfdc\uc5b4\ub0c5\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/jungle/2", "value": "^red;\ub192\uc740 \uc218\uc900\uc758 \ubc29\uc0ac\ub2a5^white;\uc774 \uc774 ^#12a003;\uc815\uae00^white; \uc9c0\uc5ed\uc744 \ub9e4\uc6b0 \uc801\ub300\uc801\uc778 \ud658\uacbd\uc73c\ub85c \ub9cc\ub4e4\uace0 \ud604\uc9c0 \uc0dd\ubb3c\ub4e4\uc744 \ud658\uc601\ubc1b\uc9c0 \ubabb\ud558\ub3c4\ub85d \ub9cc\ub4e4\uc5c8\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/tundra/0", "value": "^#61ffe3;\uc5bc\uc5b4\ubd99\uc740^white; \uc9c0\uc5ed\uc73c\ub85c, \uc5bc\uc74c\uc73c\ub85c \ub4a4\ub36e\uc5ec\uc788\uc2b5\ub2c8\ub2e4. \uc601\uad6c \ub3d9\ud1a0\uce35\uc774 \uc0c1\ub2f9\ud788 \uae4a\uc2b5\ub2c8\ub2e4. \ub208 \ubcf4\ub77c\uac00 \uc790\uc8fc \ubc1c\uc0dd\ud558\uba70, ^red;\uc628\ub3c4\uac00 \ub0ae\uc544 \ub9e4\uc6b0 \uc704\ud5d8\ud569\ub2c8\ub2e4^white;."}, {"op": "replace", "path": "/planetTypeToDescription/garden/2", "value": "\uac80\uc0ac\ud55c \ubc14\uc5d0 \ub530\ub974\uba74 \ucc29\ub959 \uc9c0\uc810\uc740 \ub098\uc058\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4 - \uc9c0\uc5ed\uc740 \uc548\uc804\ud558\uace0 \ub9ce\uc740 \uc2dd\ubb3c\uacfc \ub3d9\ubb3c\ub4e4\uc774 \uc788\uc2b5\ub2c8\ub2e4. ^#76fe68;\ud478\ub974\uace0 ^white; \ud3b8\ud55c \uc9c0\uc5ed\uc73c\ub85c \ubcf4\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/ocean/0", "value": "^#008aff;\ubc14\ub2e4^white; \uc640 \uc791\uc740 \uc12c\ub4e4\uc758 \ud589\uc131\uc785\ub2c8\ub2e4. \uc81c\uac00 \uac80\uc0ac\ud55c \ubc14\ub85c\ub294 \uc218\uc911 \uc0dd\ubb3c\uc774 \ub9ce\uace0 \ud45c\uba74\uc5d4 \uadf8\ub9bc \uac19\uc740 \uc12c\ub4e4\uc774 \uc788\uc2b5\ub2c8\ub2e4.."}, {"op": "replace", "path": "/planetTypeToDescription/tundra/1", "value": "\uc774\uacf3\uc740 ^red;\uc628\ub3c4\uac00 \ub0ae\uc544 \ub9e4\uc6b0 \uc704\ud5d8\ud569\ub2c8\ub2e4^white;. \uc774 \ucc29\ub959 \uc9c0\uc810\uc740 ^#61ffe3;\uc5bc\uc5b4\ubd99\uc740^white; \uc9c0\ub300\uc785\ub2c8\ub2e4. \ub192\uc740 \ud655\ub960\ub85c \ubd80\uc8fc\uc758\ub85c \uc778\ud55c \ub3d9\uacb0 \uc0c1\ud0dc\uc5d0 \ube60\uc9c8 \uc218 \uc788\uc2b5\ub2c8\ub2e4. \ucd94\uc704\ub85c\ubd80\ud130 \uc790\uc2e0\uc744 \ubcf4\ud638\ud558\uc138\uc694."}, {"op": "replace", "path": "/planetTypeToDescription/ocean/1", "value": "^#008aff;\ubc14\ub2e4^white; \ub85c \ub4a4\ub36e\uc778 \uc774 \ud589\uc131\uc740 \uacf3\uacf3\uc5d0 \uc12c\uc774 \ubcf4\uc774\uba70 \uc0dd\uba85\uc774 \ubc88\ucc3d\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4. \uc81c\uac00 \uac80\uc0ac\ud55c \ubc14\ub85c\ub294 \ubc14\ub2e4 \ubc11\uc5d0 \ub9ce\uc740 \ud65c\ub3d9\uc774 \uc788\ub294 \uac78\ub85c \ubcf4\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/ocean/2", "value": "\uc774 ^#008aff;\ubc14\ub2e4^white; \uc704\uc758 \ud45c\uba74\uc5d0\ub294 \ub5a0\ub2e4\ub2c8\ub294 \uc0ac\ub9c9 \uc12c\ub4e4\uc774 \ud769\ubfcc\ub824\uc838\uc788\uc2b5\ub2c8\ub2e4. \uc5f4\ub300\uc801\uc774\uace0 \uc608\uc220\uc801\uc778 \ud658\uacbd\uc744 \uac16\uace0 \uc788\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/jungle/0", "value": "\uc774 \ucc29\ub959 \uc9c0\uc810\uc740 \ubc00\uc9d1\ud55c ^#12a003;\uc815\uae00^white;\uc5d0 \uc788\uc2b5\ub2c8\ub2e4. \ubb34\uc131\ud55c \uc2dd\ubb3c\ub4e4 \uc0ac\uc774\ub85c \uc774\uc0c1\ud55c \uc0dd\ubb3c\ub4e4\uc774 \uc790\ub77c\ub098\ub294 \ub3d9\ubb3c\uc6d0\uc785\ub2c8\ub2e4. ^red;\ubc29\uc0ac\uc120 \uc218\uce58\uac00 \uadf9\ub3c4\ub85c \ub192\ub2e4^white;\ub294 \uc810\uc774 \ub9e4\uc6b0 \uc544\uc27d\uad70\uc694."}, {"op": "replace", "path": "/planetTypeToDescription/tundra/2", "value": "^red;\uc774 \ucc29\ub959 \uc9c0\uc810\uc758 \uc628\ub3c4\uac00 \ub9e4\uc6b0 \ub0ae\uc2b5\ub2c8\ub2e4^white;. \uc774 \uc9c0\uc5ed\uc740 ^#61ffe3;\uc5bc\uc5b4 \ubd99\uc5c8\uace0^white;, \ucd94\uc704\uc5d0 \uac15\ud55c \uc800\ud56d\ub825\uc744 \uac00\uc9c4 \uc2dd\ubb3c\uacfc \ub3d9\ubb3c\ub9cc\uc774 \ub0a8\uc558\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/desert/0", "value": "\uc2dd\ubb3c\uc758 \uc0dd\uc874\uc5d0 \uc81c\uc57d\uc774 \ub530\ub974\uc9c0\ub9cc, \uc774 \uc9c0\uc5ed\uc740 \uc0dd\ubb3c\uc774 \uc0b4 \uc218 \uc788\uc2b5\ub2c8\ub2e4. \ub610\ud55c, \uc774\uacf3\uc740 \ucda9\ubd84\ud55c \uc591\uc758 \ubaa8\ub798\uac00 \uc788\uc2b5\ub2c8\ub2e4. \uac04\ub2e8\ud788 \ub9d0\ud558\uc790\uba74, \uc774\uacf3\uc740 ^#e6c801;\uc0ac\ub9c9^white;\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/desert/1", "value": "\uc120\uc778\uc7a5\uc774 \uc11c\uc2dd\ud558\ub294 ^#e6c801;\uc0ac\ub9c9^white; \uc9c0\uc5ed\uc73c\ub85c, \uc5ec\ub7ec \ub3d9\ubb3c\ub4e4\ub3c4 \uc11c\uc2dd\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4. \ubaa8\ub798 \ud3ed\ud48d\uc744 \ud53c\ud558\uae30\ub9cc \ud55c\ub2e4\uba74 \uc0c1\ub300\uc801\uc73c\ub85c \uc548\uc804\ud55c \ucc29\ub959 \uc9c0\uc810\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/desert/2", "value": "\ubaa8\ub798. \ud310\ub3c5 \uacb0\uacfc \ubaa8\ub798\ubfd0\uc785\ub2c8\ub2e4. \uadf8\ub9ac\uace0 \uc57d\uac04\uc758 \uc120\uc778\uc7a5\uc774... \ud558\uc9c0\ub9cc \ubaa8\ub798\uac00 \uc555\ub3c4\uc801\uc73c\ub85c \ub9ce\uc2b5\ub2c8\ub2e4. \uc8fc\uc5b4\uc9c4 \uc815\ubcf4\uc5d0 \ub530\ub77c\uc11c \uc81c\uac00 \ucd94\uce21\ud574\ubcf4\ub2c8 \uc774 \ucc29\ub959 \uc9c0\uc810\uc740 ^#e6c801;\uc0ac\ub9c9^white;\uc5d0 \uc704\uce58\ud558\uace0 \uc788\ub294 \uac83\uc73c\ub85c \ubcf4\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/barren/0", "value": "\uc774 \uc9c0\uc5ed\uc740 ^#999966;\ucc99\ubc15^white;\ud558\uba70, \uc0dd\ubb3c\uccb4\uac00 \uc5c6\uc2b5\ub2c8\ub2e4. \ud310\ub3c5 \uacb0\uacfc \uc774\uacf3\uc5d0\ub294 \uc544\ubb34\uac83\ub3c4 \uc5c6\uc2b5\ub2c8\ub2e4. \uc815\ub9d0 \uc544\ubb34\uac83\ub3c4, \uc544\ubb34\uac83\ub3c4 \uc5c6\uc2b5\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/barren/1", "value": "\uc774 \ud589\uc131\uc740 ^#999966;\ucc99\ubc15^white;\ud569\ub2c8\ub2e4, \uc5b4\ub5a4 \uc0dd\ubb3c\uccb4\ub3c4 \uc0b4\uae30 \uc801\ud569\ud558\uc9c0 \uc54a\uc73c\uba70. \uc774\uacf3\uc5d0\ub294 \uc544\ubb34\uac83\ub3c4 \uc5c6\uc2b5\ub2c8\ub2e4..."}, {"op": "replace", "path": "/planetTypeToDescription/barren/2", "value": "\uac80\uc0ac \uacb0\uacfc \uc774 \ucc29\ub959 \uc9c0\uc810\uc740 \ud2b9\uc774\ud55c \uc0c9\uc0c1\uc744 \ub755\ub2c8\ub2e4. \ub610\ud55c, \uc774 \uacf3\uc740 ^#999966;\ucc99\ubc15^white;\ud55c \uc9c0\uc5ed\uc774\uba70 \ub530\ub77c\uc11c \uac74\ucd95\uc5d0 \uc801\ud569\ud569\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/alien/0", "value": "\uc774\uacf3\uc740 \uc0c1\ub2f9\ud788 \uc704\ud5d8\ud55c \ucc29\ub959 \uc9c0\uc810\uc785\ub2c8\ub2e4 - ^red;\ubc29\uc0ac\uc120 \uc218\uce58\uac00 \uc0c1\ub2f9\ud788 \ub192\uc2b5\ub2c8\ub2e4^white;. \ud558\uc9c0\ub9cc,  \uc774 ^#7800ff;\ubcc0\uc774^white;\ub41c \uc9c0\ud615\uc740 \ud5d8\uc545\ud558\uba74\uc11c \ub3c5\ucc3d\uc801\uc774\ub77c\uace0 \ubb18\uc0ac\ud560 \ub9cc\ub3c4 \ud569\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/alien/2", "value": "\uc774 \ucc29\ub959 \uc9c0\uc810\uc740 ^#7800ff;\ubcc0\uc774^white;\ub41c \ud658\uacbd\uc5d0 \uc790\ub9ac\uc7a1\uace0 \uc788\uc2b5\ub2c8\ub2e4. \uc774 ^red;\ub192\uc740 \uc218\uc900\uc758 \ubc29\uc0ac\uc120^white;\uc774 \uc720\ud574\ud55c \ud658\uacbd\uc744 \ub9cc\ub4e4\uc5b4\ub0c8\uc2b5\ub2c8\ub2e4. \uc218\uc601\uc744 \uc790\uc81c\ud558\ub294 \uac83\uc744 \uad8c\uace0\ud569\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/asteroids/0", "value": "\ud145 \ube48 ^#d7711e;\uc18c\ud589\uc131 \uc9c0\ub300^white;\uc785\ub2c8\ub2e4. \uac80\uc0ac \uacb0\uacfc \uc774\uacf3\uc5d0\ub294 \uc0dd\uba85\uacfc^red; \uacf5\uae30\uac00 \uc874\uc7ac\ud558\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4^white;. \ub05d\uc5c6\ub294 \uacf5\ud5c8\uc640 \uac74\ucd95\ud558\uae30 \uc88b\uc740 \uacf5\uac04\ub9cc \uc788\uc744 \ubfd0\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/asteroids/1", "value": "\uc0dd\uba85\uacfc ^red;\uacf5\uae30\uac00 \uc874\uc7ac\ud558\uc9c0 \uc54a\ub294^#d7711e;\uc18c\ud589\uc131\ub300^white;\uc785\ub2c8\ub2e4; \uc6b0\uc8fc\ub97c \ud45c\ub958\ud558\uba70, \uba3c \ubcc4\uc5d0 \uc548\uc8fc\ud558\ub294 \uadf8\ub7f0 \uacf3... \uc9d1\uc744 \uc9d3\uae30\uc5d0 \uc88b\uc740 \uc7a5\uc18c\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/asteroids/2", "value": "\ud0c0\uc6d0\ud615 \uada4\ub3c4\ub97c \ub530\ub77c\uc11c \uacf5\uc804\ud558\ub294 ^#d7711e;\uc18c\ud589\uc131\ub300^white;\uc785\ub2c8\ub2e4. \ud310\ub3c5 \uacb0\uacfc ^red;\uc774\uacf3\uc5d4 \uacf5\uae30\uac00 \uc874\uc7ac\ud558\uc9c0 \uc54a\uc2b5\ub2c8\ub2e4^white;. \uadf8\ub9ac\uace0 \ub2e4\ub978 \uc5b4\ub290 \uac83\ub3c4..."}, {"op": "replace", "path": "/planetTypeToDescription/forest/0", "value": "\ub098\ubb34\uc640 \ub2e4\ub978 \uc2dd\ubb3c\ub4e4\uc774 \ubc00\uc9d1\ud558\uac8c \uc790\ub77c\ub098\ub294, \uc774 ^#15ce02;\uc232\uc18d ^white; \ucc29\ub959 \uc9c0\uc810\uc740 \ub2e4\uc591\ud55c \uc0dd\ubb3c\uccb4\uac00 \uc790\ub77c\ub098\uae30 \uc88b\uc740 \uce5c\uc808\ud55c \uc704\uce58\uc5d0 \uc790\ub9ac\uc7a1\uace0 \uc788\uc2b5\ub2c8\ub2e4. \uc5b4\uca4c\uba74 \ub2f9\uc2e0\uc744 \ud3ec\ud568\ud574\uc11c\uc694."}, {"op": "replace", "path": "/planetTypeToDescription/forest/1", "value": "\uc78e\uc774 \ubb34\uc131\ud55c \ub098\ubb34\uac00 \ud48d\ubd80\ud55c ^#15ce02;\uc232 ^white;\uc9c0\uc5ed\uc785\ub2c8\ub2e4. \uc774 \uc9c0\uc5ed\uc740 \uc0dd\uba85\uccb4\ub97c \ub530\ub73b\ud558\uac8c \ub9de\uc774\ud574 \uc8fc\uba70, \uc81c \ub370\uc774\ud130\ubca0\uc774\uc2a4\uac00 \ub9d0\ud558\uae38 \uc774 \uc0bc\ub9bc \uc9c0\ub300\uc758 \uc0dd\uba85\uccb4\uc640 \ub9c8\uc8fc\ud560 \uc218\ub3c4 \uc788\ub2e4\uace0 \ud569\ub2c8\ub2e4."}, {"op": "replace", "path": "/planetTypeToDescription/forest/2", "value": "\uc774 \ud589\uc131\uc758 \ucc29\ub959 \uc9c0\uc810\uc740 ^#15ce02;\uc232^white;\uc5d0 \uc704\uce58\ud558\uace0 \uc788\uc2b5\ub2c8\ub2e4. \uac80\uc0ac \uacb0\uacfc \uc774 \ub098\ubb34\uac00 \uc6b0\uac70\uc9c4 \uc9c0\uc5ed\uc740 \uc0c1\ub300\uc801\uc73c\ub85c \uc870\uc6a9\ud55c \ud3b8\uc785\ub2c8\ub2e4."}, {"op": "replace", "path": "/guiConfig/windowtitle/title", "value": "\ud56d\ubc95 \ucf58\uc194"}, {"op": "replace", "path": "/planetTypeLabel/text", "value": "\ucc29\ub959 \uc9c0\uc810 \ubd84\uc11d"}, {"op": "replace", "path": "/threatLevelText/label/text", "value": "\uc704\ud5d8\ub3c4"}, {"op": "replace", "path": "/oreText/text", "value": "\uc790\uc6d0 \ubd84\uc11d"}, {"op": "replace", "path": "/sizeText/value/text", "value": "<value> \uc139\ud130"}, {"op": "replace", "path": "/sizeText/label/text", "value": "\ud589\uc131 \ud06c\uae30"}, {"op": "replace", "path": "/dayLengthText/value/text", "value": "<value> \ubd84"}, {"op": "replace", "path": "/dayLengthText/label/text", "value": "\ubc24\ub0ae \uc8fc\uae30"}, {"op": "replace", "path": "/guiConfig/windowtitle/subtitle", "value": "^#b9b5b2; \ubaa9\uc801\uc9c0\ub97c \uc120\ud0dd\ud558\uc138\uc694"}, {"op": "replace", "path": "/guiConfig/goToMyShip/caption", "value": "\ud568\uc120 \uc704\uce58\ub85c \uac00\uae30"}, {"op": "replace", "path": "/guiConfig/goToQuest/caption", "value": "\ud018\uc2a4\ud2b8 \uc704\uce58\ub85c \uac00\uae30"},